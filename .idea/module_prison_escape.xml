    </dirset>
  </path>
  
  
  <target name="compile.module.prison_escape" depends="compile.module.prison_escape.production,compile.module.prison_escape.tests" description="Compile module Prison Escape"/>
  
  <target name="compile.module.prison_escape.production" depends="register.custom.compilers" description="Compile module Prison Escape; production classes">
    <mkdir dir="${prison_escape.output.dir}"/>
    <javac2 destdir="${prison_escape.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.prison_escape}/javac">
      <compilerarg line="${compiler.args.prison_escape}"/>
      <bootclasspath refid="prison_escape.module.bootclasspath"/>
      <classpath refid="prison_escape.module.production.classpath"/>
      <src refid="prison_escape.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.prison_escape"/>
    </javac2>
    
    <copy todir="${prison_escape.output.dir}">
      <fileset dir="${module.prison_escape.basedir}/../src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.prison_escape.basedir}/../src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.prison_escape.tests" depends="register.custom.compilers,compile.module.prison_escape.production" description="compile module Prison Escape; test classes" unless="skip.tests"/>
  
  <target name="clean.module.prison_escape" description="cleanup module">
